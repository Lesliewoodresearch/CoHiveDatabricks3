{
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1.py" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
